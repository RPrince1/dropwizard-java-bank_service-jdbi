          "validationMessage": "You have entered an invalid field. Please check and try again.",
          "type": "string"
        },
        "email": {
          "title": "*Email address",
          "type": "string",
          "pattern": "^[a-zA-Z0-9.!#$%&â€™*+/=?^_`{|}~-]+@[a-zA-Z0-9-]+(?:\\.[a-zA-Z0-9-]+)*$",
          "validationMessage": "You have entered an invalid field. Please check and try again."
        },
        "password": {
          "title": "*Create a password",
          "minLength": 5,
          "validationMessage": "You have entered an invalid field. Please check and try again.",
